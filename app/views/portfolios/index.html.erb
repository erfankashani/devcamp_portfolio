<h1>Portfolio Items</h1>

<% @portfolio_items.each do |portfolio_item| %>
    <p><%= portfolio_item.title %></p> 
    <p><%= portfolio_item.subtitle if !portfolio_item.subtitle.nil? %></p> 
    <p><%= portfolio_item.body if !portfolio_item.body.nil? %></p> 
    <%= image_tag portfolio_item.thumb_image if !portfolio_item.thumb_image.nil? %>
<% end %>
